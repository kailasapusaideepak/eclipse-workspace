package Kd_Learning_POM.pageobjects;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;
import org.openqa.selenium.support.PageFactory;

import Kd_Learning.AbstractComponents.AbstractComponents;

public class ConfrimationPage extends AbstractComponents{

	WebDriver driver;
	public ConfrimationPage(WebDriver driver) {
		super(driver);
		this.driver=driver;
		PageFactory.initElements(driver, this);	
	}

	@FindBy(css=".hero-primary")
	WebElement confirmMsg;
	
	public String getCofirmMsg()
	{
		String confirmMessage=confirmMsg.getText();
		return confirmMessage;
	}
	
	
}
